//Disk Scheduling Algorithm 

//1)FCFS
#include<iostream>
using namespace std;
void FCFS(int n, int arr[], int start){
int seekCount=0;
int curr_track, distance;
for(int i=0; i<n; i++){
curr_track=arr[i];
distance=abs(start-curr_track);
seekCount+=distance;
start=curr_track;
}
cout<<"Total number of seek operations: "<<seekCount;
cout<<"seek sequence: "<<endl;
for(int i=0; i<n; i++){
cout<<arr[i];
}
}
int main(){
int n, start;
cout<<"Enter size of array: ";
cin>>n;
int arr[n];
cout<<"Enter array elements: "<<endl;
for(int i=0; i<n; i++){
cin>>arr[i];
}
cout<<"Enter the starting operation: ";
cin>>start;
FCFS(n, arr, start);
return 0;
}
